<%- include ("./partials/header") %>

<div class="container mt-3">
    <div class="w-100 text-center">
        <a href="/<%=currentUser.userType.toLowerCase()%>/<%=currentUser.userObject%>" class="btn btn-outline-info"><i class="fas fa-arrow-circle-left mr-2"></i>Return to
            Dashboard</a>
    </div>

    <h3 class="mt-3 text-center">All Registered Students</h3>
    <table class="table table-hover mt-3">
        <thead class="bg-info text-white">
            <tr>
                <th scope="col">#</th>
                <th scope="col">Student Name</th>
                <th scope="col">BlockDegree Email</th>
                <th scope="col">Contact Number</th>
                <!-- <th scope="col">University Email</th> -->
                <th></th>

            </tr>
        </thead>
        <tbody>
            <% var i=1; %>
            <% university.registeredStudentsList.forEach(function(student){ %>
            <tr class="accessRow">
                <th scope="row"><%= i %> </th>
                <td><%= student.name %></td>
                <td><%= student.blockDegreeEmail %></td>
                <td><%= student.contactNumber %></td>
            </tr>
            <% i++; }) %>
        </tbody>
    </table>
</div>

<%- include ("./partials/footer") %>